<h2>Lists</h2>

<ul>
  
  <li class="list">
    <a class="list-link" href="#/lists/<%= inbox.id %>">
      <%= inbox.get("name") %>
      <span class="task-count"><%= inbox.active_tasks.length %></span>
    </a>
  </li>
  

  <% lists.forEach(function(list){ %>
    <li class="list">
      <a class="list-link" href="#/lists/<%= list.id %>" >
        <%= list.escape('name') %>
        <span class="task-count"><%= list.active_tasks.length %></span>
      </a>
    </li>
  <% }) %>

</ul>

<a class="add-list-link" href="#/lists/new">+</a>