<form class="task-form">
  
  <input type="hidden"
    name="task[list_id]"
    value="<%= list.id %>">
  
  <input
    id="name"
    type="text"
    name="task[name]"
    placeholder="Add an item in <%= list.get('name') %>">
    
  <!--add star -->
    
  <input
    id="due-date"
    type="date"
    name="task[due_date]">
    
</form>