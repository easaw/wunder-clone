<form class="task-form">
  
  <input type="hidden"
    name="task[list_id]"
    value="<%= list.id %>">
  
  <input
    id="star"
    type="hidden"
    name="task[starred]"
    value="<%= false %>">
  
  
  
  <input
    id="name"
    type="text"
    name="task[name]"
    class="task-form-name"
    placeholder="Add an item in &quot;<%= list.get('name') %>&quot;...">
 
  <input
    id="due-date"
    class="task-form-date"
    type="date"
    name="task[due_date]">
    
  <div class="star task-form-star"></div>
    
</form>