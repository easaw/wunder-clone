<h1>Sign Up</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>
  
  <label for="name">Name</label>
  <input
    id="name"
    type="text"
    name="user[name]"
    value="<%= @user.name %>">
    
  <label for="email">Email</label>
  <input
    id="email"
    type="text"
    name="user[email]"
    value="<%= @user.email %>">
    
  <label for="password">Password</label>
  <input
    id="password"
    type="password"
    name="user[password]">
    
    
  <input type="submit">Sign Up</input>
  
</form>