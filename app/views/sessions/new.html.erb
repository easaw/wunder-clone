<h1>Sign In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  
  <label for="name">Name</label>
  <input
    id="name"
    type="text"
    name="user[name]">
    
  <br>
    
  <label for="email">Email</label>
  <input
    id="email"
    type="text"
    name="user[email]">
    
  <br>
  
  <label for="password">Name</label>
  <input
    id="password"
    type="password"
    name="user[password]">
    
  <br>
  <input type="submit" value="Sign In">
  
</form>