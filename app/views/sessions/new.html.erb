<div class="landing-page background-image">
  
  <section id="landing-modal" class="Absolute-Center">
    
    <img class="logo" src="<%= asset_url('logo.png') %>">
    
    <%= form_errors %>
    <form class="sign-in" action="<%= session_url %>" method="post">
      <%= auth_token %>
    
      <label for="email">Email</label>
      <input
        id="email"
        type="text"
        name="user[email]">
    
  
      <label for="password">Password</label>
      <input
        id="password"
        type="password"
        name="user[password]">
        
      <input type="submit" value="Sign In">
  
    </form>
    
    <button class="facebook-login hidden">Facebook</button>
    <button class= "google-login hidden">Google</button>
      
      
    <a class="back-button" href="<%= landing_url %>">Back</a>
      
  </section>
  
</div>